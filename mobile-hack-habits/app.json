{
  "expo": {
    "name": "lemon-track-habits",
    "slug": "lemon-track-habits",
    "scheme": "lemon-track-habits",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e1e1e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121212"
      }, 
      "package": "com.lemontrackhabits.lemon.dev.br"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
